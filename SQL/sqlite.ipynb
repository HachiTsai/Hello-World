    "import sqlite3\n",
    "import pandas as pd\n",
    "\n",
    "# 連接到資料庫\n",
    "conn = sqlite3.connect('../SQL/mysql/database/hi.db')\n",
    "\n",
    "# 執行 SQL 並直接顯示為表格\n",
    "df = pd.read_sql(\"SELECT * FROM users\", conn)\n",
    "df.head()\n",
    "\n",
    "# 提交修改（如 INSERT/UPDATE）\n",
    "conn.execute(\"INSERT INTO users (name) VALUES ('Alice')\")\n",
    "conn.commit()\n"
   ]
