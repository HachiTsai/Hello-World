    "#需要使用(venv)python 3.9.6\n",
    "import sqlite3\n",
    "\n",
    "# 快速測試連接\n",
    "try:\n",
    "    conn = sqlite3.connect(':memory:')  # 記憶體資料庫\n",
    "    print(\"SQLite連接成功！\")\n",
    "    conn.close()\n",
    "except sqlite3.Error as e:\n",
    "    print(f\"連接失敗: {e}\")\n"
   ]
